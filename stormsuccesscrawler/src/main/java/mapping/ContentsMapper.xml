<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapping.contentsmapper">
    <insert id="addContent" parameterType="_Content">
        insert into contents(url,title,source,pub_time,identifier,content) values(#{url},#{title},#{source},#{pub_time},#{identifier},#{content})
    </insert>
</mapper>